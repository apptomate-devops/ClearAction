.share_btns .popover {
	max-width: none;
	width: 500px;
	left: 0px !important;
	top: 0px !important;
	margin: 0;
	border-radius: 0px;
	border: 10px solid grey;
	padding: 40px;
}

.share_btns .arrow {
	display:none;
}

.share_btns h3.popover-title {
	font-weight: normal;
	color: #2d4977;
	padding: 0 0 20px 0px;
	width: 75%;
	margin: 0;
	font-size: 24px;
	background: none;
	border: none;
	border-radius: 0;
	line-height: 30px;
}

.share_btns .popover-content {
	border-top: 5px solid #6992c0;
	padding-top: 5px;
}

.share_btns .popover-content h4 {
	color: #064b7b;
	font-size: 19px;
	font-weight: normal;
	font-family: 'Montserrat', sans-serif;
	margin-bottom: 20px;
}

.share_popover {
	display:none;
}

.share_btns .popover-content button {
	font-size: 16px;
	line-height: 16px;
}

.share_btns .caret {
	 margin-left: 30px;
	border-top: 6px dashed;
    border-right: 6px solid transparent;
    border-left: 6px solid transparent;
}

.share_btns ul {
	background: #e6e6e6;
	border: 1px solid #c3d3e6;
	border-radius: 0;
	width: 60%;
}

.share_btns ul li {
	list-style: none !important;
	border-bottom: 1px solid #c3d3e6;
	margin: 5px 0px !important;
	padding: 10px 18px;
	color: #064b7b !important;
	font: 14px normal 'Montserrat', sans-serif;
}

.share_btns ul li:last-of-type {
	border:none;
}

.share_btns .popover-content .close {
    margin-top: -155px;
    border: 1px solid #5093c3;
    height: 32px;
    border-radius: 50%;
    width: 32px;
    margin-right: -25px;
    color: #5093c3;
    opacity: 1;
    font-size: 50px;
    font-weight: 100;
	line-height: 10px;
	font-family: 'Montserrat', sans-serif;
}

.share_btns .connection_cnt ul, .share_btns .connection_cnt li {
	list-style: none;
	border: none;

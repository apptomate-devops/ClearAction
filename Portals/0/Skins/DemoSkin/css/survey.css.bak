.exercise .body ol {
	counter-reset: list;
}

.exercise .body ol li:before {
	content: "(" counter(list) ") ";
  counter-increment: list;
}

.exercise .body ul {
	margin: 0px auto 30px;
}

.exercise .body ul li {

    }
.exercise .progress_bar {
	color: #666666;
	font-size: 14px;
	margin-top: 40px;
	background: url('../images/separator.png') repeat-x 0px 15px ;
}

.exercise .progress_bar a {
	color: #666666;
}

.exercise .progress_bar a:hover {
	color: #5093c3;
}

.exercise .progress_bar li:first-of-type:after {
	background: url('../images/separator-end.png') no-repeat 0px 0px;
	width:5px;
	height:5px;
	position:relative;
	top:-41px;
	left: -13px;
	content: '';
	display:block;
}

.exercise .progress_bar li:last-of-type:after {
	background: url('../images/separator-end.png') no-repeat 0px 0px;
	width:5px;
	height:5px;
	position:relative;
	top:-41px;
	left: 110px;
	content: '';
	display:block;
}

.exercise .progress_bar li {
	list-style: none;
	float: left;
	width: 16.66%;
	position: relative;
	text-align: center;
	padding: 0px 11px;
}
.exercise .progress_bar li:before {
	content: ' ';
	width: 30px;
	height: 30px;
	border: 1px solid #a7a7a7;
	background-color: white;
	border-radius: 50%;
	display: block;
	text-align: center;
	margin: 0 auto 5px;
}
.exercise .progress_bar li.active:before {
	background-color: #6992c0;
}
.exercise .progress_bar span {
	display: inline-block;
	margin-top: 3px;
}
.exercise .body {
	padding: 70px 40px 40px 40px;
	border-bottom: 1px solid #f1f1f1;
}

